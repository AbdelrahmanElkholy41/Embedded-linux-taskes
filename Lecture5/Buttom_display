import tkinter as tk
from tkinter import StringVar
from tkinter import Entry, Label



# Initialize the main window
root = tk.Tk()
root.geometry("300x300")

buttom=tk.Button(root,text="Buttom1")
buttom.grid(row=0,column=1)

buttom=tk.Button(root,text="Buttom2")
buttom.grid(row=1,column=0)

buttom=tk.Button(root,text="Buttom3")
buttom.grid(row=1,column=2)

buttom=tk.Button(root,text="Buttom4")
buttom.grid(row=2,column=1)
root.mainloop()
